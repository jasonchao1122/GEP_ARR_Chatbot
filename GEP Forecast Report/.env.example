# GEP Forecast Report Environment Variables
# Copy this file to .env and fill in your values

# Slack Bot Token (required)
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token-here

# Slack Channel (optional, defaults to #emb-auto-growth-updates)
SLACK_CHANNEL=#emb-auto-growth-updates

# Snowflake Configuration (optional, defaults are provided)
SNOWFLAKE_USER=your.email@gusto.com
SNOWFLAKE_ACCOUNT=gusto.us-west-2
SNOWFLAKE_WAREHOUSE=ANALYTICS_WH
SNOWFLAKE_DATABASE=ANALYTICS
SNOWFLAKE_SCHEMA=GROWTH
SNOWFLAKE_ROLE=ANALYTICS_ROLE
